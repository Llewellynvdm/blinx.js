{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "comment-format": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "no-empty": [true, "allow-empty-catch"]
  },
  "linterOptions": {
    "exclude": ["src/lib/**", "coverage/**", "assets/**"]
  }
}

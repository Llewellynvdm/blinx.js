{
  "name": "blinx.js",
  "version": "0.0.1",
  "description": "A multi-language client-side script to automatically convert Bible references to Bible links with a passage pop-up",
  "main": "dist/blinx.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/",
    "build": "webpack --config webpack.config.js",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js"
  },
  "keywords": [
    "bible",
    "reference",
    "tagger"
  ],
  "author": "Rene Hamburger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/renehamburger/blinx.js.git"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "@types/node": "9.4.6",
    "awesome-typescript-loader": "3.4.1",
    "jasmine-core": "2.99.1",
    "karma": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "2.0.9",
    "source-map-loader": "0.2.3",
    "tslint": "5.9.1",
    "tslint-config-standard": "7.0.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.7.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  },
  "dependencies": {
    "umbrellajs": "2.10.1"
  }
}
